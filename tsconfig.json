{
	"version": "1.6.2",
	"compilerOptions": {
		"declaration": false,
		"module": "umd",
		"noImplicitAny": true,
		"outDir": "_build/",
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"moduleResolution": "node"
	},
	"filesGlob": [
		"./src/**/*.ts",
		"./tests/**/*.ts",
		"./_modules/**/*.d.ts",
		"./typings/tsd.d.ts"
	],
	"files": [
		"./typings/tsd.d.ts",
		"./src/cellSelection.ts",
		"./src/decorator.ts",
		"./src/editor.ts",
		"./src/grid.ts",
		"./src/list.ts",
		"./src/pagination.ts",
		"./src/selection.ts",
		"./src/main.ts",
		"./node_modules/dojo-compose/typings/dojo-compose/dojo-compose-2.0.0-beta.1.d.ts",
		"./node_modules/dojo-compose/typings/node/node.d.ts",
		"./node_modules/dojo-core/typings/dojo-core/dojo-core-2.0.0-alpha.3.d.ts"
	],
	"exclude": []
}
